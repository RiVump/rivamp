{
	"name": "rivump/rivump",
	"description": "Composer Library",
	"type": "project",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "Romantsov Roman",
			"email": "mchack4@gmail.com"
		},
		{
			"name": "Vladislav Borisenko",
			"email": "invinciblewest@gmail.com"
		}
	],
	"require-dev": {
		"symfony/var-dumper": "4.4"
	},
	"autoload": {
		"psr-4": {
			"RiVump\\": "rivump/class/"
		},
		"files": [
			"rivump/helpers/helpers.php"
		]
	},
	"require": {
		"php": "^7.3",
		"ext-pdo": "*",
		"ext-json": "*",
		"ext-curl": "*",
		"guzzlehttp/guzzle": "7.0",
		"illuminate/collections": "^8.34",
		"league/flysystem": "^2.0",
		"symfony/cache": "^5.2",
		"monolog/monolog": "^2.2",
		"rivump/rest": "^1.0",
		"rivump/config": "^1.0",
		"rivump/debugbar": "^1.0",
		"phpunit/phpunit": "^9.5"
	},
	"license": "MIT",
	"scripts": {
		"post-create-project-cmd": [
			"@php -r \"unlink('.gitignore');\"",
			"@php -r \"unlink('LICENSE');\"",
			"@php -r \"unlink('README.md');\"",
			"rd /s /q \"./test\""
		]
	}
}
